{
  "name": "xy-parser",
  "version": "5.0.2",
  "description": "Parse a text-file and convert it to an array of XY points",
  "main": "./lib/index.js",
  "module": "./lib-esm/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "src",
    "lib-esm"
  ],
  "scripts": {
    "build-doc": "cheminfo doc",
    "check-types": "tsc --noEmit",
    "clean": "rimraf lib lib-esm",
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "prepack": "npm run tsc",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "test": "npm run test-only && npm run eslint && npm run check-types",
    "test-coverage": "jest --coverage",
    "test-only": "jest",
    "tsc": "npm run clean && npm run tsc-cjs && npm run tsc-esm",
    "tsc-cjs": "tsc --project tsconfig.cjs.json",
    "tsc-esm": "tsc --project tsconfig.esm.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/xy-parser.git"
  },
  "keywords": [
    "xy file parser",
    "mass spectroscopy"
  ],
  "author": "Luc Patiny",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/xy-parser/issues"
  },
  "homepage": "https://github.com/cheminfo/xy-parser#readme",
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "cheminfo-tools": "^1.23.3",
    "codecov": "^3.8.3",
    "eslint": "^8.28.0",
    "eslint-config-cheminfo-typescript": "^11.2.2",
    "jest": "^29.3.1",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "cheminfo-types": "^1.4.0",
    "ensure-string": "^1.2.0",
    "ml-spectra-processing": "^11.14.0"
  }
}
