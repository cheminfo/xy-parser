{
  "name": "xy-parser",
  "version": "5.0.5",
  "description": "Parse a text-file and convert it to an array of XY points",
  "main": "./lib/index.js",
  "module": "./lib-esm/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "src",
    "lib-esm"
  ],
  "scripts": {
    "build": "npm run tsc-esm && cheminfo-build --entry lib-esm/index.js --root XYParser",
    "build-doc": "cheminfo doc",
    "check-types": "tsc --noEmit",
    "clean": "rimraf lib lib-esm",
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "prepack": "npm run tsc",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "test": "npm run test-only && npm run eslint && npm run prettier && npm run check-types",
    "test-only": "jest --coverage",
    "tsc": "npm run clean && npm run tsc-cjs && npm run tsc-esm",
    "tsc-cjs": "tsc --project tsconfig.cjs.json",
    "tsc-esm": "tsc --project tsconfig.esm.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/xy-parser.git"
  },
  "keywords": [
    "xy file parser",
    "mass spectroscopy"
  ],
  "author": "Luc Patiny",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/xy-parser/issues"
  },
  "homepage": "https://github.com/cheminfo/xy-parser#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "cheminfo-build": "^1.2.0",
    "cheminfo-tools": "^1.23.3",
    "codecov": "^3.8.3",
    "eslint": "^8.57.0",
    "eslint-config-cheminfo-typescript": "^12.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "cheminfo-types": "^1.7.2",
    "ensure-string": "^1.2.0",
    "ml-spectra-processing": "^14.2.0"
  }
}
